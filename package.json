{
  "name": "grid-together",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "nodemon server/dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "react:dev": "if-env DEV_ENV=cloud && npm run react:dev-cloud || npm run react:dev-local",
    "react:dev-cloud": "cd client && (env DANGEROUSLY_DISABLE_HOST_CHECK=true && yarn start)",
    "react:dev-local": "cd client && yarn start",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\""
  },
  "repository": "git@github.com:Thunderducky/grid-together.git",
  "author": "Eric Scott <scottzillaster@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/ws": "^7.2.6",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "ws": "^7.3.1"
  }
}
